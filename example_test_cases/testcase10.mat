# Lets try some operations with vectors

vector hello[5]
scalar i
scalar n
scalar result 

i = 1
n = 6
hello = {1 3 5 6 8}

for(i in 1:n:1) {
  hello = hello * i
  result = result + hello[1] + hello[4]
  print(result)
}

# Output is
# 7
# 21
# 21 + 6 + 42 = 69
# 69 + 24 + 168 = 261
# 261 + 120 + 840 = 1221
# 1221 + 720 + 5040 = 6981